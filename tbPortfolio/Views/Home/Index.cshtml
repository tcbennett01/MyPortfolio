

    <!-- ==============================================
    HEADER
    =============================================== -->
    <header id="home" class="jumbotron bg-image">
        <div class="container">
            <div class="row">
                <div class="col-sm-12 text-col text-center">
                    <h1 id="myIntro" style="font-size: 18px">Hi, I am</h1>
                    <h1>Tim Bennett</h1>
                    <p id="myTitle">Full-Stack Software Engineer</p><br />
                    @*<h4 class="stackFadeIn" style="display: none">Stack Matters...</h4>*@
                    <div class="bsCarousel">
                        <div id="scrollCarousel" class="carousel slide stackFadeIn" data-ride="carousel" data-interval="4500" style="display: none">

