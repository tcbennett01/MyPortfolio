                                <div class="item">SQL - SSRS - SSDT</div>
                                <div class="item">Cognos - SAS - Tableau</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>

@section scripts{
    <script>
        $(function () {
            $('.stackFadeIn').delay(1500).fadeIn(1000);

        });
    </script>
